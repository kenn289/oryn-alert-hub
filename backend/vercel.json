{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "dist/server-simple.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server-simple.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/server-simple.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "PORT": "3002"
  }
}
